<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.centit.mallserver.dao.ShoppingMembershipDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.centit.mallserver.po.ShoppingMembershipPo">
        <result column="normal" property="normal" />
        <result column="silver" property="silver" />
        <result column="gold" property="gold" />
        <result column="diamond" property="diamond" />
    </resultMap>

    <!--  查询详情 -->
    <select id="selectDetail" resultMap="BaseResultMap" >
            select t.* from shopping_membership t limit 1
    </select>

</mapper>
